  })\n        }, [])\n        return data\n    }\n\n    const usePost = resource => {\n\n        const [data, dispatch] = useReducer(reducer, INITIAL_STATE)\n      \n        const post = data => {\n          dispatch({type: 'REQUEST'})\n          axios\n            .post(baseURL + resource + '.json', data)\n            .then(res => {\n              dispatch({type: 'SUCCESS',\n              data: res.data\n            })\n              console.log(res.data)\n          })\n        }\n      \n        return [data, post]\n    }\n\n    const useDelete = () => {\n        const [data, dispatch] = useReducer(reducer, INITIAL_STATE)\n      \n        const remove = resource => {\n          dispatch({type: 'REQUEST'})\n          axios\n            .delete(baseURL + resource + '.json')\n            .then(() => {\n              dispatch({type: 'SUCCESS'})\n          })\n        }\n        \n        return [data, remove]\n    }\n\n    return {\n        useGet,\n        usePost,\n        useDelete\n    }\n}\n\n\nexport default init","/home/pedro-martins/devpleno/my-money/src/App.jsx",["20","21","22"],{"ruleId":"23","severity":1,"message":"24","line":37,"column":12,"nodeType":"25","endLine":37,"endColumn":14,"suggestions":"26"},{"ruleId":"27","severity":1,"message":"28","line":7,"column":16,"nodeType":"29","messageId":"30","endLine":7,"endColumn":23},{"ruleId":"27","severity":1,"message":"31","line":7,"column":25,"nodeType":"29","messageId":"30","endLine":7,"endColumn":34},{"ruleId":"32","severity":1,"message":"33","line":25,"column":11,"nodeType":"34","endLine":25,"endColumn":39},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'resource'. Either include it or remove the dependency array.","ArrayExpression",["35"],"no-unused-vars","'usePost' is assigned a value but never used.","Identifier","unusedVar","'useDelete' is assigned a value but never used.","jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Pr